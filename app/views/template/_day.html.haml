%table{:border => 1}  
  %tr
    %td{:colspan => 4}
      &nbsp;
  %tr
    %td{:colspan => 4}
      &nbsp;
  - for row in (ALL_TIMES.index(START_TIME)-1)..(ALL_TIMES.index(END_TIME)-1)
    %tr
      - for x in ['slotAs', 'slotBs', 'slotCs', 'slotDs']
        %td{:class => "td_24"}<
          - if day == nil
            &nbsp;
          - else
            = render :partial => 'temp_select', :locals => {:slot => eval("day.#{x}[row]"), :col => x, :row => row}