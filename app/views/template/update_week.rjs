for x in 1..7
  dtemp = @wtemplate.days[x-1] 
  unless dtemp.nil?
    page.replace_html "day#{x}".to_sym, {:partial => "dtemp", :locals => {:dtemp => dtemp}}
  else
    page.replace_html "day#{x}".to_sym, {:partial => "dtemp", :locals => {:dtemp => nil}}
  end
#  page.replace_html "day#{x}".to_sym, "day: #{@wtemplate.days[x-1]}, x #{x}, dtemp #{@wtemplate[@wtemplate.days[x-1]]}"
end